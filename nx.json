{
  "npmScope": "abgov",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "core-css": {
      "tags": []
    },
    "angular-components": {
      "tags": [],
      "implicitDependencies": ["shared-common"]
    },
    "shared-storybook-common": {
      "tags": []
    },
    "vue-components": {
      "tags": [],
      "implicitDependencies": ["shared-common"]
    },
    "shared-common": {
      "tags": []
    }
  }
}
