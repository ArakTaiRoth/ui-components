{
  "version": 1,
  "projects": {
    "core-css": {
      "root": "libs/core-css",
      "sourceRoot": "libs/core-css/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/core-css/tsconfig.lib.json",
              "libs/core-css/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core-css/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core-css/jest.config.js",
            "tsConfig": "libs/core-css/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/core-css",
            "tsConfig": "libs/core-css/tsconfig.lib.json",
            "packageJson": "libs/core-css/package.json",
            "main": "libs/core-css/src/index.ts",
            "assets": ["libs/core-css/*.md"]
          }
        }
      }
    },
    "angular-components": {
      "projectType": "library",
      "root": "libs/angular-components",
      "sourceRoot": "libs/angular-components/src",
      "prefix": "goa-dio",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/angular-components/tsconfig.lib.json",
            "project": "libs/angular-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/angular-components/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/angular-components/tsconfig.lib.json",
              "libs/angular-components/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/angular-components/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/angular-components/jest.config.js",
            "tsConfig": "libs/angular-components/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/angular-components/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  }
}
