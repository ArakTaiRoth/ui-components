  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "build:all": "nx run-many --target=build --all",
    "build:npm": "nx run-many --target=build --projects=angular-components,core-css,vue-components --parallel --prod",
    "build:storybook": "run-p build:core-storybook build:angular-storybook build:vue-storybook",
    "build:angular-components": "npm run build angular-components --withDeps --prod",
    "build:vue-components": "npm run build vue-components --withDeps --prod",
    "build:core-css": "npm run build core-css --withDeps --prod",
    "build:plugin:vue": "nx run vue-plugin:build",
    "build:angular-storybook": "nx run angular-components:build-storybook",
    "build:core-storybook": "nx run core-css:build-storybook",
    "build:vue-storybook": "nx run vue-components:build-storybook",
    "test": "nx test --clearCache",
    "test:all": "nx run-many --target=test --all",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "publish:npm-test": "run-p \"publish:npm:* --test\"",
    "publish:npm": "run-p publish:npm:*",
    "publish:script": "node publish.js --path $npm_config_path --test=$npm_config_test",
    "publish:npm:angular-components": "npm run publish:script --path=dist/libs/angular-components",
    "publish:npm:core-css": "npm run publish:script --path=dist/libs/core-css",
    "publish:npm:vue-components": "npm run publish:script --path=dist/libs/vue-components",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "node node_modules/@nx-plus/vue/patch-nx-dep-graph.js",
    "semantic-delivery": "semantic-delivery-gitlab"
  },
